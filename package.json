{
  "name": "@a1motion/defer",
  "version": "1.0.7",
  "main": "lib/index.js",
  "private": false,
  "repository": "https://github.com/a1motion/defer",
  "maintainers": [
    {
      "name": "Connor Love",
      "email": "connor@a1motion.com"
    }
  ],
  "license": "MIT",
  "scripts": {
    "prepublish": "yarn build",
    "build": "tsc",
    "test": "jest --env=node --ci"
  },
  "files": [
    "lib/*",
    "LICENSE",
    "README.md"
  ],
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "10.14.6",
    "jest": "24.8.0",
    "jest-junit": "6.4.0",
    "ts-jest": "24.0.2",
    "tslint": "5.16.0",
    "typescript": "3.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  }
}